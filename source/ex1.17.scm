(define (multi a b)
  (if (= b 0)
      0
    (+ a (multi a (- b 1)))
    )
  )
(define (double a)
  (+ a a)
  )
(define (halve a)
  (/ a 2)
  )
(define (mul-iter a b res)
  (if (= b 0)
      res
    (if (even? b)
        (mul-iter (double a) (/ b 2)  res )
      (mul-iter a (- b 1) (+ res a))
      )
    )
  )
(define (mul a b)
  (mul-iter a b 0)
  )

